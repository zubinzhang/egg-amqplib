    "eggPlugin",
    "egg-plugin",
    "amqp",
    "amqplib"
  ],
  "dependencies": {
    "@types/amqplib": "^0.5.13",
    "amqplib": "^0.5.5",
    "bluebird": "^3.5.5"
  },
  "devDependencies": {
    "autod": "^3.1.0",
    "autod-egg": "^1.1.0",
    "egg": "^2.23.0",
